{
  "name": "battlescribe2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "fast-xml-parser": "^3.12.14",
    "flow-bin": "^0.89.0",
    "he": "^1.2.0",
    "lodash": "^4.17.11",
    "multer": "^1.4.1",
    "react": "^16.8.4",
